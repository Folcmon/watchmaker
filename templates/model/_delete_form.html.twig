<form method="post" action="{{ path('app_model_delete', {'id': model.id}) }}"
      onsubmit="return confirm('{{ 'Czy na pewno usunąć'|trans }}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ model.id) }}">
    <button class="btn btn-danger btn-lg">{{ "Usuń"|trans }}</button>
</form>