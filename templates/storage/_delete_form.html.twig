<form method="post" action="{{ path('storage_delete', {'id': storage.id}) }}"
      onsubmit="return confirm('{{ 'Czy na pewno usunąć'|trans }}');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ storage.id) }}">
    <button class="btn btn-danger btn-lg">{{ "Usuń"|trans }}</button>
</form>
