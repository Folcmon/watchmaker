{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "api-platform/core": "v4.0.17",
    "beberlei/doctrineextensions": "*",
    "composer/package-versions-deprecated": "1.11.99.5",
    "doctrine/annotations": "2.0.2",
    "doctrine/doctrine-bundle": "^2.10",
    "doctrine/doctrine-migrations-bundle": "^3.2",
    "doctrine/orm": "3.3.0",
    "knplabs/knp-paginator-bundle": "*",
    "phpdocumentor/reflection-docblock": "^5.3",
    "stof/doctrine-extensions-bundle": "^1.8",
    "symfony/asset": "7.3.*",
    "symfony/asset-mapper": "^7.3",
    "symfony/console": "7.3.*",
    "symfony/dotenv": "7.3.*",
    "symfony/expression-language": "7.3.*",
    "symfony/flex": "^2.4",
    "symfony/form": "7.3.*",
    "symfony/framework-bundle": "7.3.*",
    "symfony/http-client": "7.3.*",
    "symfony/http-foundation": "^7.3",
    "symfony/intl": "7.3.*",
    "symfony/mailer": "7.3.*",
    "symfony/messenger": "^7.3",
    "symfony/mime": "7.3.*",
    "symfony/monolog-bundle": "^3.8",
    "symfony/notifier": "7.3.*",
    "symfony/process": "7.3.*",
    "symfony/property-access": "7.3.*",
    "symfony/property-info": "7.3.*",
    "symfony/proxy-manager-bridge": "v6.4.13",
    "symfony/security-bundle": "7.3.*",
    "symfony/serializer": "7.3.*",
    "symfony/string": "7.3.*",
    "symfony/translation": "7.3.*",
    "symfony/twig-bundle": "^7.3",
    "symfony/validator": "7.3.*",
    "symfony/web-link": "7.3.*",
    "symfony/yaml": "7.3.*",
    "twig/extra-bundle": "^2.12|v3.20.0",
    "twig/string-extra": "^v3.20.0",
    "twig/twig": "^2.12|v3.20.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "3.6.1",
    "phpstan/phpstan": "1.12.10",
    "rector/rector": "1.2.10",
    "symfony/browser-kit": "^7.3",
    "symfony/css-selector": "^7.3",
    "symfony/debug-bundle": "^7.3",
    "symfony/maker-bundle": "^1.0",
    "symfony/phpunit-bridge": "^7.3",
    "symfony/stopwatch": "^7.3",
    "symfony/test-pack": "v1.1.0",
    "symfony/var-dumper": "^7.3",
    "symfony/web-profiler-bundle": "^7.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "allow-plugins": {
      "symfony/flex": false
    },
    "platform": {
      "php": "8.2"
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "php bin/console cache:clear",
      "assets:install %PUBLIC_DIR%": "php bin/console assets:install"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "7.3.*"
    }
  }
}
